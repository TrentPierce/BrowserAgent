{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature", "enhancement", "feat"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["bug", "fix", "bugfix"]
    },
    {
      "title": "## ğŸ“š Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## ğŸ”§ Maintenance",
      "labels": ["chore", "maintenance", "refactor"]
    },
    {
      "title": "## ğŸ”’ Security",
      "labels": ["security"]
    },
    {
      "title": "## âš¡ Performance",
      "labels": ["performance", "perf"]
    }
  ],
  "ignore_labels": [
    "ignore",
    "wontfix",
    "duplicate",
    "invalid"
  ],
  "sort": "ASC",
  "template": "#{{CHANGELOG}}\n\n**Full Changelog**: #{{RELEASE_DIFF}}\n",
  "pr_template": "- #{{TITLE}} by @#{{AUTHOR}} in ##{{NUMBER}}",
  "empty_template": "No changes in this release",
  "label_extractor": [
    {
      "pattern": "^feat(\\(.+\\))?:",
      "target": "feature"
    },
    {
      "pattern": "^fix(\\(.+\\))?:",
      "target": "bug"
    },
    {
      "pattern": "^docs(\\(.+\\))?:",
      "target": "documentation"
    },
    {
      "pattern": "^chore(\\(.+\\))?:",
      "target": "chore"
    }
  ]
}
